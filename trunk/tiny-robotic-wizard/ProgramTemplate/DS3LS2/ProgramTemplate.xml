<?xml version="1.0" encoding="utf-8"?>
<ProgramTemplate name="DS3LS2" description="距離センサ:3段階, ラインセンサ:2つ" xmlns="http://tiny-robotic-wizard.googlecode.com/trw1">
	<context>
		<status name="DistanceSensor" caption="測距センサ" image=".\DistanceSensor3.png" code=".\DistanceSensor.c">
			<matter name="missing" caption="見あたらない" />
			<matter name="near" caption="近い" />
			<matter name="middle" capiton="中間" />
			<matter name="far" caption="遠い" />
		</status>
		<status name="LineSensor" caption="ラインセンサ" image=".\LineSensor2.png" code=".\LineSensor.c">
			<matter name="0" caption="" />
			<matter name="1" caption="" />
			<matter name="2" caption="" />
			<matter name="3" caption="" />
		</status>
	</context>
	<actions>
		<action name="LED" caption="LED" image=".\LED.png" code=".\LED.c">
			<procedure name="black" caption="消灯" />
			<procedure name="blue" caption="青" />
			<procedure name="green" caption="緑" />
			<procedure name="cyan" caption="シアン" />
			<procedure name="red" caption="赤" />
			<procedure name="magenta" caption="マゼンタ" />
			<procedure name="yellow" caption="黄" />
			<procedure name="white" caption="白" />
		</action>
		<action name="Ambulation" caption="移動" image=".\Ambulation.png" code=".\Ambulation.c">
			<procedure name="nop" caption="何もしない" />
			<procedure name="forward" caption="前進" />
			<procedure name="back" caption="後進" />
			<procedure name="cw" caption="右回り" />
			<procedure name="ccw" caption="左回り" />
			<procedure name="stop" caption="停止" />
		</action>
	</actions>
</ProgramTemplate>
