<?xml version="1.0" encoding="utf-8"?>
<ProgramTemplate Name="DS2LS1" Description="距離センサ:2段階, ラインセンサ:1つ" xmlns="http://tiny-robotic-wizard.googleCode.com/">
	<Input>
		<Device Name="DistanceSensor" Caption="測距センサ" Image=".\DistanceSensor2.png" Code=".\DistanceSensor.c">
			<Option Name="missing" Caption="見あたらない" />
			<Option Name="near" Caption="近い" />
			<Option Name="far" Caption="遠い" />
		</Device>
		<Device Name="LineSensor" Caption="ラインセンサ" Image=".\LineSensor1.png" Code=".\LineSensor.c">
			<Option Name="0" Caption="" />
			<Option Name="1" Caption="" />
		</Device>
	</Input>
	<Output>
		<Device Name="LED" Caption="LED" Image=".\LED.png" Code=".\LED.c">
			<Option Name="black" Caption="非点灯" />
			<Option Name="blue" Caption="青" />
			<Option Name="green" Caption="緑" />
			<Option Name="cyan" Caption="シアン" />
			<Option Name="red" Caption="赤" />
			<Option Name="magenta" Caption="マゼンタ" />
			<Option Name="yellow" Caption="黄" />
			<Option Name="white" Caption="白" />
		</Device>
		<Device Name="Ambulation" Caption="移動" Image=".\Ambulation.png" Code=".\Ambulation.c">
			<Option Name="nop" Caption="何もしない" />
			<Option Name="forward" Caption="前進" />
			<Option Name="back" Caption="後進" />
			<Option Name="cw" Caption="右回り" />
			<Option Name="ccw" Caption="左回り" />
			<Option Name="stop" Caption="停止" />
		</Device>
	</Output>
</ProgramTemplate>
